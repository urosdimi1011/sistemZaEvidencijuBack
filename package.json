  "main": "index.js",
  "config": {
    "migration_name": ""
  },
  "scripts": {
    "build": "tsc",
    "start": "set NODE_ENV=production && npm run build&& node dist/index.js",
    "dev": "nodemon --watch src --ext ts,js --exec ts-node src/index.ts",
    "typeorm": "ts-node ./node_modules/typeorm/cli.js",
    "migration:generate": "npm run typeorm -- -d src/data-source.ts migration:generate",
    "migration:run": "npm run typeorm -- -d src/data-source.ts migration:run",
    "migration:revert": "npm run typeorm -- -d src/data-source.ts migration:revert",
    "seed": "ts-node src/seed/seedSchoolsAndOccupations.ts",
    "seed:schools": "ts-node src/seed/seedSchoolsAndOccupations.ts"
  },
