    "@types/jsonwebtoken": "^9.0.10",
    "bcryptjs": "^3.0.2",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.0",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.25",
    "yup": "^1.7.0"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.9",
